{
  "name": "code16/gum",
  "description": "",
  "authors": [{
    "name": "Philippe Lonchampt",
    "email": "philippe@code16.fr"
  }],
  "require": {
    "php": ">=7.4",
    "laravel/framework": "^7.0|^8.0",
    "laravel/legacy-factories" : "v1.1.0",
    "erusev/parsedown": "^1.7",
    "moontoast/math": "^1.1"
  },
  "require-dev": {
    "algolia/algoliasearch-client-php": "^2.2",
    "laravel/scout": "^8.0",
    "code16/sharp": "^6.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "1.3.*",
    "phpunit/phpunit": "^8.5",
    "doctrine/dbal": "~2.5",
    "orchestra/testbench": "5.*|6.*"
